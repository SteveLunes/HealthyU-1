%h1 Enter New Muscle Exercise

= form_tag exercises_path do

  = label :exercise, :name, 'name'
  = text_field :exercise, 'name'
  
  = label :exercise, :muscle, 'muscle'
  = text_field :exercise, 'muscle'

  = label :exercise, :description, 'description'
  = text_field :exercise, 'description'

  = submit_tag 'Save Exercise'