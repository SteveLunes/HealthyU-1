<% provide(:title, 'All users') %>
<h1>All users</h1>

<ul class="users">
  <% @users.each do |user| %>
    <li>
      <%= gravatar_for user, size: 50 %>
      <%= user.name %>
    </li>
  <% end %>
</ul>
<li><%= link_to "Profile", current_user %></li>
<li><%= link_to "Log out", logout_path, method: :delete %></li>